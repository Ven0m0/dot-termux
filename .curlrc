### General ###
silent
show-error
progress-bar
# follow redirects
location
# Wait before timing out.
connect-timeout 15
create-dirs
skip-existing
fail
fail-early

# HTTP
tls-max 1.3
http3
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp
doh-insecure

# Compression
compressed
compressed-ssh
parallel

# When following a redirect, automatically set the previous URL as referer.
referer ";auto"

# Continue transfer
continue-at -

# Use the server-provided timestamp, if any.
remote-time
remote-name-all
# remote-name
suppress-connect-headers
